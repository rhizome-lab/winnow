    "imports": [{ "names": ["createHarloweRuntime"], "path": "harlowe/runtime" }],
    "entry": "const _rt = createHarloweRuntime();\n{userScripts}\n_rt.Engine.registerStoryletsConditions({storylets});\n_rt.start({passages}, {startPassage}, {passageTags}, { persistence: {persistence}, sourceMap: {passageSources} });",
    "dependencies": {
      "keybinds": "github:pterror/keybinds"
    }
  },
  "class_preamble": null,
  "type_definitions": {},
  "function_signatures": {},
  "function_modules": [
    {
      "names": ["Colors", "Collections", "StringOps"],
      "path": "harlowe/engine"
    }
  ]
}
