{
  "system_modules": {
    "Harlowe.State": { "path": "harlowe/state", "stateful": true },
    "Harlowe.H": { "path": "harlowe/context" },
    "Harlowe.Navigation": { "path": "harlowe/navigation", "stateful": true },
    "Harlowe.Engine": { "path": "harlowe/engine", "stateful": true },
    "Harlowe.Audio": { "path": "harlowe/audio", "stateful": true }
  },
  "runtime_type": {
    "name": "HarloweRuntime",
    "path": "harlowe/runtime"
  },
  "context_type": {
    "name": "HarloweContext",
    "path": "harlowe/context"
  },
