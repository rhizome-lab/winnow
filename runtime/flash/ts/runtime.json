{
  "system_modules": {
    "Flash.Object": { "path": "flash/object" },
    "Flash.Class": { "path": "flash/class" },
    "Flash.Scope": { "path": "flash/scope" },
    "Flash.Exception": { "path": "flash/exception" },
    "Flash.Iterator": { "path": "flash/iterator" },
    "Flash.Memory": { "path": "flash/memory" },
    "Flash.XML": { "path": "flash/xml" },
    "Flash.Utils": { "path": "flash/utils" }
  },
  "scaffold": {
    "imports": [
      { "names": ["stage", "flashTick"], "path": "flash/runtime" }
    ],
    "construct_class_init": "stage.addChild(app);",
    "tick": "flashTick();"
  },
  "class_preamble": {
    "names": ["QN_KEY", "registerClass", "registerClassTraits"],
    "path": "flash/utils"
  }
}
